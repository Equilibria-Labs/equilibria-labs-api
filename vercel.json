{
  "version": 2,
  "buildCommand": "pnpm --filter=@equilibria-labs/web build",
  "outputDirectory": "apps/web/.next",
  "framework": "nextjs",
  "installCommand": "pnpm install",
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "projects": {
    "@equilibria-labs/web": {
      "rootDirectory": "apps/web"
    }
  },
  "build": {
    "env": {
      "TURBO_REMOTE_ONLY": "true"
    }
  }
}
