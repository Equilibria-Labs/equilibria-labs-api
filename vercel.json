{
  "version": 2,
  "devCommand": "pnpm --filter=@equilibria-labs/web run dev -- --port $PORT",
  "buildCommand": "cd ../../ && pnpm turbo build --filter=@equilibria-labs/web...",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/.next/server/pages/$1"
    }
  ]
}
